{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": null,
  "startCommand": "npm start", 
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "https://taste-of-home-client.vercel.app"
      },
      "dest": "server.js"
    }
  ]
}